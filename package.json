{
  "name": "react-component-boilerplate",
  "description": "Boilerplate for React.js components",
  "author": "Juho Vepsalainen",
  "user": "survivejs",
  "version": "0.6.0",
  "scripts": {
    "start": "TARGET=dev node lib/dev_server.js",
    "icon": "node lib/icon_importer.js",
    "install": "node lib/icon_importer.js && node_modules/lodash-cli/bin/lodash modularize modern strict exports=es -o app/lib/_",
    "build": "TARGET=public webpack --colors"
  },
  "main": "dist-modules/index.js",
  "dependencies": {
    "babel-core": "^5.4.7",
    "babel-loader": "^5.1.3",
    "html-webpack-plugin": "^1.4.0",
    "leaflet": "^1.0.0-beta.1",
    "lodash-cli": "^3.10.0",
    "material-design-icons": "^2.0.0",
    "react": "^0.13.3",
    "webpack": "^1.9.10",
    "webpack-merge": "^0.1.1"
  },
  "devDependencies": {
    "react-hot-loader": "^1.2.7",
    "webpack-dev-server": "^1.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://git.becoast.fr/react-boilerplate.git"
  },
  "license": "MIT"
